import csv

INPUT_CSV = "CQs HLS avec doublon_20260115163713.csv"
OUTPUT_CSV = "CQs_HLS_sans_doublon.csv"

seen = set()
unique_rows = []

with open(INPUT_CSV, newline="", encoding="utf-8") as f:
    reader = csv.reader(f)
    header = next(reader)  # garder l'en-t√™te
    for row in reader:
        row_tuple = tuple(row)
        if row_tuple not in seen:
            seen.add(row_tuple)
            unique_rows.append(row)

with open(OUTPUT_CSV, "w", newline="", encoding="utf-8") as f:
    writer = csv.writer(f)
    writer.writerow(header)
    writer.writerows(unique_rows)

print(f"‚úî CSV nettoy√© : {len(unique_rows)} lignes uniques")
print(f"üìÅ Fichier g√©n√©r√© : {OUTPUT_CSV}")